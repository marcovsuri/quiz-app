<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.Region?>
<VBox alignment="CENTER" spacing="20" xmlns="http://javafx.com/javafx/23.0.1"
      xmlns:fx="http://javafx.com/fxml/1"
      fx:controller="org.roxburylatin.advcompsci.quizapp.application.teacher.CompletedTabController"
      styleClass="tab-content">
    <padding>
        <Insets top="20" right="20" bottom="20" left="20"/>
    </padding>

    <ListView fx:id="studentListView" prefHeight="250.0" maxHeight="250.0" prefWidth="600.0" styleClass="list-view"/>

    <Region VBox.vgrow="ALWAYS"/>

    <HBox alignment="CENTER" spacing="20" VBox.vgrow="NEVER">
        <Button fx:id="clearButton" text="Clear" styleClass="clear"/>
    </HBox>
</VBox>
